{
  "name": "tsdown-workspace-demo",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsgo",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@lib/aurora": "workspace:*",
    "@lib/nebula": "workspace:*",
    "@lib/zenith": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@typescript/native-preview": "7.0.0-dev.20260227.1",
    "tsdown": "^0.21.0-beta.2"
  },
  "pnpm": {
    "overrides": {
      "rolldown": "1.0.0-rc.6"
    }
  }
}
